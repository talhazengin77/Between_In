SELECT title,length FROM film
WHERE length BETWEEN 90 AND 120

SELECT title,length FROM film
WHERE NOT length BETWEEN 90 AND 120

SELECT rental_rate, replacement_cost FROM film
WHERE (rental_rate BETWEEN 2 AND 4) AND (replacement_COST BETWEEN 10 AND 20)

SELECT LENGTH FROM film
WHERE length IN (49,50,60) -- ARALIK DEĞİL SATIRLARI TEK TEK ALIYORUZ.

--ÖDEV 2 
--1 
SELECT * FROM film 
WHERE replacement_cost BETWEEN 12.99 AND 16.99

--2 
SELECT first_name,last_name FROM actor
WHERE first_name IN ('Penelope') OR first_name IN ('Nick') OR first_name IN ('Ed')

--3
SELECT * FROM film
WHERE rental_rate IN (0.99,2.99,4.99) AND replacement_cost IN (12.99,15.99,28.99)